  keywords = {BaaS,blockchain,blockchain as a service,Blockchain-as-a-Service,cloud,cloud computing,Cloud computing,Contracts,data privacy,distributed ledger technologies,distributed ledger technology,DLT,electronic money,financial data processing,governance,law,Protocols,security,service provider,trust,trusted computing}
