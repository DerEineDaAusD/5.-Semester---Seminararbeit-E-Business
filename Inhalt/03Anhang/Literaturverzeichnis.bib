  journal = {IEEE Access},
  keywords = {Bitcoin system,Blockchain,blockchain network,blockchain service,blockchain system,blockchain technology,blockchain-as-a-service,blockchain-as-a-service platform,cloud computing,Cloud computing,cryptography,distributed databases,Monitoring,NutBaaS,Peer-to-peer computing,service-oriented architecture,smart contracts,Smart contracts}
